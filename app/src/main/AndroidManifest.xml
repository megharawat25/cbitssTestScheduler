<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vikaskumar.examschedulercbitss" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/cbitss"
        android:label="@string/app_name"
        android:roundIcon="@drawable/cbitss"
        android:supportsRtl="true"
        android:theme="@style/Theme.ExamSchedulerCbitss" >
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/Theme.ExamSchedulerCbitss.NoActionBar" >
        </activity>
        <activity android:name=".AdminActivity" />
        <activity android:name=".SplashScreen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
    </application>

</manifest>